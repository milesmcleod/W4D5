<h1> <a href="<%= band_url(band.id) %>"><%= band.name %></a></h1>
<br>
<ul>
<% band.albums.each do |album| %>
  <li><a href="<%= album_url(album) %>"><%= album.title %></a></li>
<% end %>
</ul>
<br>
<%= link_to "delete this funky band", band_url(band), method: :delete %>
<br>
<%= link_to "edit this funky band", edit_band_url(band)%>
